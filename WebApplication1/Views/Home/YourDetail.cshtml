@model Customer
@{
	Layout = "Layout2";
}
<div class="container">
	<div class="row" style="padding-top: 0px;">
		<div class="heading-container-aboutUs" style="justify-content: center;">
			<p class="welcome" style="padding-left: 50PX;color:white;">Your Personal Information</p>
		</div>
	</div>
</div>

<div class="login-container py-5">

	<div class="row" style="justify-content:center;">
		<h1 style="font-family:'Times New Roman', Times, serif;font-style:italic;">Customer Detail</h1>
	</div>
	<span>
		<h4>Name:</h4>
		<p>@Model.Name</p>
	</span>
	<span>
		<h4>Address:</h4>
		<p>@Model.Address</p>
	</span>
	<span>
		<h4>Phone Number:</h4>
		<p>@Model.PhoneNumber</p>
	</span>

	<span>
		<h4>Email:</h4>
		<p>@Model.Email</p>
	</span>


</div>


