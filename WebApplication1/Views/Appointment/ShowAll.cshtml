
@model List<Appointment>
@{
	Layout = "Layout2";
}

<div style="margin-top: 100px;">

	<div class="table-responsive">

	<table class="table table-striped">
  <thead>
    <tr>
        <th>ID</th>
      <th>Name</th>
      <th>Phone number</th>
      <th>Pet Type</th>
      <th>Preferred Date and Time</th>
      <th>Reason</th>
    </tr>
  </thead>

@foreach (var app in Model)
{
	


  <tbody>
    <tr>
      <td>@app.Id</td>
      <td>@app.Name</td>
      <td>@app.Phone</td>
      <td>@app.PetType</td>
      <td>@app.preferredDateTime</td>
      <td>@app.Reason</td>
    </tr>
  </tbody>


}
</table>

</div>

</div>





